#include<stdio.h>
#include<conio.h>
int tiemkiemtuyentinh(int a[], int x, int n)
{
    for(int i = 0; i < n;i++)
    {
        if(a[i] == x)
        return x;
    }
    return 0;
}
int main()
{
    int a[] = {2, 3, 4, 10, 40};
    int n = sizeof(a) / sizeof(a[0]);
    int x = 10; 
    printf("Gia tri 10 tai phan tu thu: %d", tiemkiemtuyentinh(a, n, x));
    return 0;
}
